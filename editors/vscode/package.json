{
  "name": "oxide-analyzer",
  "displayName": "oxide-analyzer",
  "description": "Oxide language support for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:oxide"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "oxide",
        "extensions": [
          ".ox"
        ],
        "aliases": [
          "Oxide",
          "oxide",
          "ox"
        ]
      }
    ]
  },
  "oxide-analyzer.trace.server": {
    "scope": "window",
    "type": "string",
    "enum": [
      "off",
      "messages",
      "verbose"
    ],
    "default": "messages"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
